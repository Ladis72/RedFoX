}
BarraBusqueda{
    background-color: rgba(170, 170, 127,0);
}

QTableView::header
{
    color: #363b31;
}
QTableView
{
    color :#000000;
}

*[codeField="true"]
{
    background-color: #f3eea6;
    color:black;
}

*[locateField="true"]
{
    background-color: #e7e5bf;
    color:black;
}

QTextEdit{
    border:1px solid grey;
}

QStatusBar::item {
    border-width: 1;
    border-color: darkkhaki;
    border-style: solid;
    border-radius: 2;
}

tab {
    border-radius:5px;

}

QTabWidget::tab-bar {
    left: 5px;

    /* move to the right by 5px */
}

QTabBar::tab
{
    background: #304163;
    color: #FFFFFF;
    /* same as the pane color */
    border-top-left-radius: 4px;
    border-top-right-radius: 4px;
    min-width: 1ex;
    padding: 5px;
    margin-right:5px;

}

QTabBar::tab:hover
{
    color:#a7c65c;
}
QTabWidget::tab
{
    background-color : #000000;
}

QTabBar::tab:selected
{
    color: #ffcc02;

}

QTabBar::tab:!selected
{
    margin-top: 1px;
    /* make non-selected tabs look smaller */
}

QTabBar::scroller
{
    /* the width of the scroll buttons */
    width: 15px;
    border-image: url(:Icons/images/spinup.png) ;
}

QTabBar QButton
{
    /* the scroll buttons are tool buttons */
    border-image: url(:Icons/images/spinup.png) ;
        border-width: 0px;
}

QTabWidget::up-button
{
    subcontrol-origin: border;
    subcontrol-position: top right;
    width: 16px; /* 16 + 2*1px border-width = 15px padding + 3px parent border */
    border-image: url(:Icons/images/spinup.png) ;
    border-width: 1px;
}

QTabWidget::up-button:hover
{
    border-image: url(:Icons/images/spinup_hover.png) ;
}

QTabWidget::up-button:pressed
{
    border-image: url(:Icons/images/spinup_pressed.png) ;
}

QTabWidget::down-button
{
    subcontrol-origin: border;
    subcontrol-position: bottom right;

    width: 16px;
    border-image: url(:Icons/images/pushbutton.png) ;
    border-width: 1px;
    border-top-width: 0;
}

QListView
{
       show-decoration-selected: 1;
}

QListView::item:hover
{
    background-color: Yellow;
    color:blue;
}

 QFrame {
     border: 1px solid grey;
     border-radius: 4px;
 }
 QLabel
 {
    border:0px;
 }



QLineEdit:focus
{
    border-width: 1.5px;
    padding: 0px;
}


QCheckBox
{
    spacing: 5px;
}

QCheckBox::indicator,QGroupBox::indicator
{
    width: 13px;
    height: 13px;
}

QCheckBox::indicator:unchecked,QGroupBox::indicator:unchecked {
    image: url(:Icons/images/checkbox_unchecked.png);
}

QCheckBox::indicator:unchecked:hover,QGroupBox::indicator:unchecked:hover {
    image: url(:Icons/images/checkbox_unchecked_hover.png);
}

QCheckBox::indicator:unchecked:pressed,QGroupBox::indicator:unchecked:pressed {
    image: url(:Icons/images/checkbox_unchecked_pressed.png);
}

QCheckBox::indicator:checked,QGroupBox::indicator:checked {
    image: url(:Icons/images/checkbox_checked.png);
}

QCheckBox::indicator:checked:hover,QGroupBox::indicator:checked:hover {
    image: url(:Icons/images/checkbox_checked_hover.png);
}

QCheckBox::indicator:checked:pressed,QGroupBox::indicator:checked:pressed {
    image: url(:Icons/images/checkbox_checked_pressed.png);
}

QGroupBox::title {
     subcontrol-origin: margin;
     subcontrol-position: top center; /* position at the top center */
     padding: 0 3px;
 }
 QGroupBox {
      border: 1px solid gray;
      border-radius: 5px;
      margin-top: 7px; /* leave space at the top for the title */
}
QDialogButtonBox
{
      button-layout: 0;
}


QTableView, QTableWidet
{
     color:rgb(0,0,0);
     selection-background-color:#4476C1;
     alternate-background-color: rgb(255, 255, 127);
 }
QMenuBar
{
    background-color: rgb(170, 170, 127);
     color: rgb(0,0,0);
}

QMenuBar::item
{
    spacing: 3px; /* spacing between menu bar items */
    padding: 1px 4px;
    background: transparent;
    color: black;
    border-radius: 4px;
 }

QMenuBar::item:selected
{
    /* when selected using mouse or keyboard */
    background-color: #ABABAB;
    color: white;
}
QMenuBar::item:pressed
{
     background: #2d2f23;
}

QMenu
{
     background-color: rgb(170, 170, 127);
     border: 1px solid black;
}

QMenu::item
{
      /* sets background of menu item. set this to something non-transparent
          if you want menu color and menu item color to be different */
      background-color: transparent;
      color: black;
}

QMenu::item:selected
{ /* when user selects item using mouse or keyboard */
      background-color: rgb(170, 170, 127);
      color: white;
}

QToolTip
{
    border: 2px solid darkkhaki;
    padding: 5px;
    background-color:yellow;
    color:black;
    border-radius: 3px;
    opacity: 200;
}
