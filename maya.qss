QLabel , QStatusBar, QLineEdit, QDateEdit, QTextEdit,QTableWidget,QTableView, QSpinBox,
QDoubleSpinBox, QComboBox, QHeaderView, QGroupBox, QListView, QRadioButton
{
    font: normal;
    font: 10pt "Cantarell";


}
QTextEdit:enabled, QSpinBox,QDoubleSpinBox, QComboBox, QLineEdit, QDateEdit, QDateTimeEdit
    {
    color: black;
   /* background-color:#f6f6f0;*/
}
QLineEdit
{
       border-radius: 5px;
}

QScrollBar:vertical
{
    border: none;
    background: #32CC99;
    width: 10px;
    margin: 12px 0 12px 0;
}
QScrollBar::handle:vertical {
    background: #506d76;
    border-radius:5;
    min-height: 10px;
}
QScrollBar::add-line:vertical {
    border: none solid grey;
    background: #32CC99;
    height: 10px;
    subcontrol-position: bottom;
    subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
    border: none solid grey;
    background: #32CC99;
    height: 10px;
    subcontrol-position: top;
    subcontrol-origin: margin;
}
QScrollBar::up-arrow:vertical {
    border: 2px solid grey;
    height: 10px;
    background: white;
    width: 10px;
    image: url(:/Icons/images/scroll_up.png) ;
}
QScrollBar::down-arrow:vertical{
      border: 2px solid grey;
      height: 10px;
      background: white;
      width: 10px;
      image: url(:/Icons/images/scroll_down.png) ;
  }

QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

QScrollBar:horizontal
{
    border: none solid grey;
    background: #32CC99;
    height: 10px;
    margin: 0px 10px 0px 10px;
}

QScrollBar::handle:horizontal
{
      background: #506d76;
      border-radius:5;
      min-width: 10px;
}

QScrollBar::add-line:horizontal
{
      border: none solid grey;
      background: #32CC99;
      width: 10px;
      subcontrol-position: right;
      subcontrol-origin: margin;
}

QScrollBar::sub-line:horizontal
{
      border: none solid grey;
      background: #32CC99;
      width: 10px;
      subcontrol-position: left;
      subcontrol-origin: margin;
}
QScrollBar::add-page:horizontal, QScrollBar::sub-page:horizontal
{
      background: none;
}

QScrollBar:left-arrow:horizontal
{
       border: none solid grey;
       height: 10px;
       background: white;
       width: 10px;
       image: url(:/Icons/images/left_arrow.png) ;
}
QScrollBar::right-arrow:horizontal
{
    border: none solid grey;
    width: 10px;
    height: 10px;
    background: white;
    image: url(:/Icons/images/right_arrow.png) ;
}

