QTabWidget::up-button {
    subcontrol-origin: border;
    subcontrol-position: top right;
    width: 16px; /* 16 + 2*1px border-width = 15px padding + 3px parent border */
    border-image: url(:Icons/images/spinup.png) 1;
    border-width: 1px;
}

QTabWidget::up-button:hover {
    border-image: url(:Icons/images/spinup_hover.png) 1;
}

QTabWidget::up-button:pressed {
    border-image: url(:Icons/images/spinup_pressed.png) 1;
}

QtabWidget::down-button {
    subcontrol-origin: border;
    subcontrol-position: bottom right;

    width: 16px;
    border-image: url(:Icons/images/pushbutton.png) 1;
    border-width: 1px;
    border-top-width: 0;
}


QPushButton {
    min-width: 1.5em;
}

QPushButton, QComboBox[editable="false"],
QComboBox[editable="true"]::drop-down {
    border-image: url(:Icons/images/pushbutton.png) 5;
    border-width: 5;
}

QPushButton:hover, QComboBox[editable="false"]:hover,
QComboBox[editable="true"]::drop-down:hover, QMenuBar::item:hover {
    border-image: url(:Icons/images/pushbutton_hover.png) 5;
    border-width: 5;
}

QPushButton:pressed, QComboBox[editable="false"]:on,
QComboBox[editable="true"]::drop-down:on, QMenuBar::item:on {
    border-image: url(:Icons/images/pushbutton_pressed.png) 5;
    border-width: 5;
}


/* Customize check boxes. */
QCheckBox {
    spacing: 5px;
}

QCheckBox::indicator {
    width: 13px;
    height: 13px;
}

QCheckBox::indicator:unchecked {
    image: url(:Icons/images/checkbox_unchecked.png);
}

QCheckBox::indicator:unchecked:hover {
    image: url(:Icons/images/checkbox_unchecked_hover.png);
}

QCheckBox::indicator:unchecked:pressed {
    image: url(:Icons/images/checkbox_unchecked_pressed.png);
}

QCheckBox::indicator:checked {
    image: url(:Icons/images/checkbox_checked.png);
}

QCheckBox::indicator:checked:hover {
    image: url(:Icons/images/checkbox_checked_hover.png);
}

QCheckBox::indicator:checked:pressed {
    image: url(:Icons/images/checkbox_checked_pressed.png);
}

/* Force the dialog's buttons to follow the Windows guidelines. */
QDialogButtonBox {
    button-layout: 0;
}


  QScrollBar:vertical {
      border: none;
      background: #32CC99;
      width: 20px;
      margin: 22px 0 22px 0;
  }
  QScrollBar::handle:vertical {
      background: blue;
      border-radius:5;
      min-height: 20px;
  }
  QScrollBar::add-line:vertical {
      border: none solid grey;
      background: #32CC99;
      height: 20px;
      subcontrol-position: bottom;
      subcontrol-origin: margin;
  }

  QScrollBar::sub-line:vertical {
      border: none solid grey;
      background: #32CC99;
      height: 20px;
      subcontrol-position: top;
      subcontrol-origin: margin;
  }
  QScrollBar::up-arrow:vertical {
      border: 2px solid grey;
      height: 20px;
      background: white;
      width: 20px;
      border-image: url(:Icons/images/spinup.png) 2;
  }
  QScrollBar::down-arrow:vertical{
        border: 2px solid grey;
        height: 20px;
        background: white;
        width: 20px;
        border-image: url(:Icons/images/spindown.png) 2;
    }

  QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
      background: none;
  }

